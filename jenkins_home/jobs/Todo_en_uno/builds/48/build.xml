    [name: &apos;Herman&apos;, Ciudad: &apos;Bogota&apos;, Carrera: &apos;Softeware Engineer&apos;,Dinero:5076],
    [name: &apos;Carlos&apos;, Ciudad: &apos;La paz&apos;, Carrera: &apos;Civil Engineer&apos;,Dinero: 6789],
