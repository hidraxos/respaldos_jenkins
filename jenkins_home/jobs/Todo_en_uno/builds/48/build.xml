    }
    parameters{
            choice(name: &apos;NAMES&apos;, choices: [&quot;Herman&quot;,&quot;Carlos&quot;,&quot;Beatriz&quot;,&quot;Gladys&quot;])
            booleanParam(name:&apos;Refresh_Param&apos;,description: &apos;&apos;, defaultValue: false)
