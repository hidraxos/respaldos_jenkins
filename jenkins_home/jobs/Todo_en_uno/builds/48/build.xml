                        withCredentials([usernamePassword(credentialsId: credentials, passwordVariable: &apos;PASS&apos;, usernameVariable:&apos;USERNAME&apos;)]){
                            sh&quot;sshpass -p ${PASS} ssh ${USERNAME}@${ip_ubuntu} hostname &gt;&gt; ${file}&quot;
                        }
                    }
                }
            }
        }
    }
    post{
